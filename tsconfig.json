{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "paths": {
      "@domain/*": ["./domain/*"],
      "@presentation/*": ["./presentation/*"],
      "@utils/*": ["./utils/*"],
      "@utils": ["./utils"],
      "@types/*": ["./types/*"],
      "@types": ["./types"],
      "@enums/*": ["./enums/*"],
      "@enums": ["./enums"]
    }
  },
  "exclude": ["bkp"]
}
