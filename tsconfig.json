{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "paths": {
      "@data/*": ["./data/*"],
      "@domain/*": ["./domain/*"],
      "@main/*": ["./main/*"],
      "@presentation/*": ["./presentation/*"]
    }
  },
  "exclude": ["bkp"]
}
